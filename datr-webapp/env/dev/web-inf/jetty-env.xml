<?xml version="1.0" encoding="UTF-8"?>
<Configure class="org.eclipse.jetty.webapp.WebAppContext">
	<Set name="contextPath">/datr</Set>
	<New id="testds" class="org.eclipse.jetty.plus.jndi.Resource">
		<Arg></Arg>
		<Arg>java:comp/env/jdbc/testds</Arg>
		<Arg>
			<New class="org.apache.commons.dbcp2.BasicDataSource">
				<Set name="driverClassName">org.hsqldb.jdbc.JDBCDriver</Set>
				<Set name="url">jdbc:hsqldb:db/test</Set>
				<Set name="username">SA</Set>
				<Set name="password"></Set>
			</New>
		</Arg>
	</New>
</Configure>